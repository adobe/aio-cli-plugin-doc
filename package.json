{
  "name": "@adobe/aio-cli-plugin-doc",
  "homepage": "https://github.com/adobe/aio-cli-plugin-doc",
  "repository": "https://github.com/adobe/aio-cli-plugin-doc",
  "bugs": {
    "url": "https://github.com/adobe/aio-cli-plugin-doc/issues"
  },
  "version": "1.2.1",
  "dependencies": {
    "@adobe/aio-lib-core-config": "^1.2.8",
    "@adobe/aio-lib-core-logging": "^1.1.0",
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.1.2",
    "cli-ux": "^5.4.6",
    "execa": "^4.0.2",
    "gatsby": "^2.23.21",
    "yeoman-environment": "^2.5.0",
    "yeoman-generator": "^5.9.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-aio-lib-config": "^2.0.1",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "acorn": "^7",
    "chalk": "^4",
    "eslint": "^8.47.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsdoc": "^42.0.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^29",
    "jest-haste-map": "^24.5.0",
    "jest-junit": "^6.0.0",
    "jest-resolve": "^24.5.0",
    "stdout-stderr": "^0.1.9",
    "typescript": "^4.9.5",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.6.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "aio",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "doc": {
        "description": "Create and build a new Adobe I/O doc site"
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>"
  },
  "main": "src/index.js",
  "scripts": {
    "pretest": "eslint src test e2e",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "e2e": "jest --collectCoverage=false --testRegex './e2e/e2e.js'"
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
